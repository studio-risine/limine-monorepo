{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["./paths-alias.json", "./base.json"],
	"compilerOptions": {
		"plugins": [{ "name": "next" }],
		"lib": ["es2022", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowJs": true,
		"jsx": "preserve",
		"noEmit": true
	}
}
